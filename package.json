{
  "name": "ask11-storage",
  "version": "0.1.0",
  "main": "storage.js",
  "repository": "git://github.com:ask11/storage.git",
  "author": "Aleksey Kulikov <alekseys.kulikov@gmail.com> (http://twitter.com/ask_11)",
  "license": "MIT",
  "scripts": {
    "postinstall": "command -v component > /dev/null && component install && npm run build",
    "test": "testem -f test/testem.json ci",
    "start": "testem -f test/testem.json",
    "build": "component build",
    "stat": "cloc lib/ --quiet --exclude-dir=vendor --by-file && cloc test/*.js --quiet --by-file"
  },
  "devDependencies": {
    "component": "*",
    "testem": "*",
    "async": "*",
    "chai": "*"
  },
  "testling": {
    "preprocess": "npm run build",
    "html": "test/test-runner.html",
    "browsers": [
      "ie/6..latest",
      "firefox/3..5",
      "firefox/19..nightly",
      "chrome/4..7",
      "chrome/25..canary",
      "opera/10..next",
      "safari/4..latest",
      "iphone/6",
      "ipad/6",
      "android/4.2"
    ]
  }
}
